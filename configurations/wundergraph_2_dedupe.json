{
    "version": "1",
    "execution_config": {
        "file": {
            "path": "./config.json"
        }
    },
    "engine": {
        "enable_single_flight": true,
        "enable_request_tracing": false,
        "enable_persisted_operations_cache": false,
        "enable_normalization_cache": false,
        "enable_validation_cache": false,
        "operation_hash_cache_size": 0,
        "execution_plan_cache_size": 0,
        "normalization_cache_size": 0,
        "validation_cache_size": 0,
        "max_concurrent_resolvers": 200
    },
    "traffic_shaping": {
        "all": {
            "keep_alive_idle_timeout": "60s",
            "keep_alive_probe_interval": "60s"
        }
    },

    "log_level": "panic",
    "json_log": false,
    "query_plans_enabled": false,
    "cache_control_policy": {
        "enabled": false,
        "value": "max-age=180, public"
    },
    "persisted_operations": {
        "cache": {
            "size": "10MB"
        }
    },
    "telemetry": {
        "metrics": {
            "prometheus": {
                "enabled": false
            }
        }
    }
}
