{
  "version": "1",
  "execution_config": {
    "file": {
      "path": "./wundergraph_config.json"
    }
  },
  "cache_control_policy": {
    "enabled": true,
    "value": "max-age=180, public"
  },
  "persisted_operations": {
    "cache": {
      "size": "100MB"
    }
  },
  "engine": {
    "enable_single_flight": true,
    "enable_persisted_operations_cache": true,
    "enable_normalization_cache": true,
    "enable_validation_cache": true,
    "operation_hash_cache_size": 10000,
    "execution_plan_cache_size": 10000,
    "normalization_cache_size": 10000,
    "validation_cache_size": 10000,
    "minify_subgraph_operations": true,
    "enable_request_tracing": false,
    "max_concurrent_resolvers": 200
  },
  "traffic_shaping": {
    "all": {
      "keep_alive_idle_timeout": "60s",
      "keep_alive_probe_interval": "60s"
    }
  },
  "router_registration": false,
  "log_level": "error",
  "json_log": false,
  "query_plans_enabled": false,
  "graphql_metrics": {
    "enabled": false
  },
  "telemetry": {
    "tracing": {
      "enabled": false
    },
    "metrics": {
      "prometheus": {
        "enabled": false
      },
      "otlp": {
        "enabled": false
      }
    }
  }
}
